static u16_t
lwip_standard_chksum(void *dataptr, u16_t len)
{
  u32_t acc;
  u16_t src;
  u8_t *octetptr;

  acc = 0;
   
  octetptr = (u8_t*)dataptr;
  while (len > 1) {
    
 
    src = (*octetptr) << 8;
    octetptr++;
     
    src |= (*octetptr);
    octetptr++;
    acc += src;
    len -= 2;
  }
  if (len > 0) {
     
    src = (*octetptr) << 8;
    acc += src;
  }
   
  acc = (acc >> 16) + (acc & 0x0000ffffUL);
  if ((acc & 0xffff0000UL) != 0) {
    acc = (acc >> 16) + (acc & 0x0000ffffUL);
  }
  

 
  return htons((u16_t)acc);
}


